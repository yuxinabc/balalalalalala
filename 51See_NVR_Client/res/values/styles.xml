<?xml version="1.0" encoding="utf-8"?>
<resources>
    <style name="ExampleTheme" parent="Theme.AppCompat.Light.NoActionBar">
  <!--  <item name="android:homeAsUpIndicator">@drawable/back_img</item> 设置actionbar左上角图标-->       
    </style>
    
        <style name="StyledIndicators.ActionBar"  parent="@android:style/Widget.Holo.Light.ActionBar">
        <item name="android:displayOptions">showHome</item> 
        <item name="android:icon">@drawable/content_menu</item>
        <item name="android:background">@color/white</item>
        <item name="android:height">45dp</item>
        <item name="android:titleTextStyle">@style/MyActionBarTitleText</item>
    </style>    
    <!--ActionBar title style-->
    <style name="MyActionBarTitleText" >
        <item name="android:textSize">18sp</item>  
        <item name="android:textColor">#FFFFFF</item>  
    </style>
    
    
	<style name="guidebk" parent="android:Theme">
	    <item name="android:windowBackground">@drawable/bg_add</item>
	    <item name="android:windowNoTitle">true</item>
	</style>
	<style name="style0">
		<item name="android:textSize">16sp</item>
		<item name="android:textColor">#FFFFFF</item>
	</style>
	<style name="style1">
		<item name="android:textSize">12sp</item>
		<item name="android:textColor">#616263</item>
	</style>
	<style name="style2">
		<item name="android:textSize">18sp</item>
		<item name="android:textStyle">bold</item>
	</style>
	<style name="style3">
		<item name="android:textSize">24sp</item>
		<item name="android:textColor">#AA003D</item> 
		<!--<item name="android:textColor">#000000</item> -->
	</style>
    <style name="style5">
		<item name="android:textSize">24sp</item>
		<!-- <item name="android:textColor">#CCCCCC</item> -->
	</style>
	<style name="btnback">
		<item name="android:textSize">20sp</item>
		<item name="android:textColor">#FFFFFF</item>
	</style>
		<style name="btnrefresh">
		<item name="android:textSize">10sp</item>
		<item name="android:textColor">#FFFFFF</item>
	</style>
	<style name="btnControl">
		<item name="android:textSize">20sp</item>
		<item name="android:textColor">#FF00003D</item>
	</style>
	<style name="btnset">
		<item name="android:textSize">23sp</item>
		<item name="android:textColor">#AA00003D</item>
	</style>
	<style name="usercentertitle">
		<item name="android:textSize">23sp</item>
		<item name="android:textStyle">bold</item>
		<item name="android:textColor">#FFFFFF</item>
	</style>

	<style name="style4" parent="style2">
		<item name="android:shadowColor">#0F0</item>
		<item name="android:shadowDx">0</item>
		<item name="android:shadowDy">0</item>
		<item name="android:shadowRadius">10</item>
	</style>
	
	<style name="MyCheckBox" parent="@android:style/Widget.CompoundButton.CheckBox">
        <item name="android:textSize">16.0sp</item>
        <item name="android:paddingLeft">28.0px</item>
        <item name="android:button">@drawable/btn_check</item>
    </style>
    
    <style name="styleEditText">
		<item name="android:textSize">12sp</item>
		<item name="android:textColor">#7F7F7F</item>
	</style>
	
	<style name="registerRetTitle">
		<item name="android:textSize">23sp</item>
		<item name="android:textColor">#7F7F7F</item>
	</style>
	
	<style name="mainPanelTitle" parent="android:Theme">
       <item name="android:windowTitleSize">50dip</item>
    </style>
	<style name="CustomWindowTitleBackground">
  		<item name="android:background">#565656</item> 
  	</style>
	<style name="titlebar_style" parent="android:Theme">
  		<item name="android:windowTitleSize">50dp</item> 
  		<item name="android:windowTitleBackgroundStyle">@style/CustomWindowTitleBackground</item> 
  	</style>
  	
	<style name="textFont">
        <item name="android:textSize">15sp</item>
        <item name="android:textColor">#000000</item>
    </style>
    <style name="dialog_animation_style" parent="android:Animation">  
        <item name="android:windowEnterAnimation">@anim/dialog_enter</item>
        <item name="android:windowExitAnimation">@anim/dialog_exit</item>
    </style>  
    
	<style name="loadingDialogStyle" parent="android:Theme.Dialog">
        <item name="android:windowBackground">@android:color/transparent</item>
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowIsFloating">true</item>
        <item name="android:windowIsTranslucent">true</item>
        <item name="android:windowContentOverlay">@null</item>
        <item name="android:windowAnimationStyle">@android:style/Animation.Dialog</item>
        <item name="android:backgroundDimEnabled">true</item>
    </style>
    <style name="NumberProgressBar_Default">
        <item name="android:layout_height">wrap_content</item>
        <item name="android:layout_width">match_parent</item>
        <item name="max">100</item>
        <item name="progress">0</item>
        <item name="progress_unreached_color">#CCCCCC</item>
        <item name="progress_reached_color">#3498DB</item>
        <item name="progress_text_size">10sp</item>
        <item name="progress_text_color">#3498DB</item>
        <item name="progress_reached_bar_height">3dp</item>
        <item name="progress_unreached_bar_height">2dp</item>
    </style>
    
    <style name="WifiDialogStyle" parent="android:style/Theme.Dialog">  
        <item name="android:background">#00000000</item>  
        <item name="android:windowBackground">@android:color/transparent</item>  
        <item name="android:windowNoTitle">true</item>  
        <item name="android:windowIsFloating">true</item>  
    </style> 
    
    <style name="Erro_Dialog"  parent="@android:style/Theme.Dialog"> 
         <!-- 设置浮动窗口 -->
        <item name="android:windowIsFloating">true</item>
        <!--背景颜色及透明程度--> 
        <item name="android:windowBackground">@android:color/transparent</item>
        <!-- 无边框设置 -->
        <item name="android:windowFrame">@null</item>
        <!-- 无标题栏设置 -->
        <item name="android:windowNoTitle">true</item>
        
        <!-- 设置完全透明 -->
        <item name="android:background">@color/transparent</item>
    </style>
    <style name="rf_control_dialog" 
        parent="@android:style/Theme.Dialog">  
        <item name="android:windowBackground"> @android:color/transparent </item>  
        <item name="android:backgroundDimEnabled">false</item>  
        <item name="android:windowFrame">@null</item>
        <item name="android:windowNoTitle">true</item>
        <item name="android:background">@color/transparent</item>  
        </style>   
    <style name="Login_Dialog"  parent="@android:style/Theme.Dialog"> 
         <!-- 设置浮动窗口 -->
        <item name="android:windowIsFloating">true</item>
        <!--背景颜色及透明程度--> 
        <item name="android:windowBackground">@android:color/transparent</item>
        <!-- 无边框设置 -->
        <item name="android:windowFrame">@null</item>
        <!-- 无标题栏设置 -->
        <item name="android:windowNoTitle">true</item>
        <!-- 设置完全透明 -->
        <item name="android:background">@color/transparent</item>
    </style>
    
    <style name="RadioButton">
        <item name="android:textColor">@drawable/button_text_color</item>
        <item name="android:minHeight">33dp</item>
        <item name="android:minWidth">70dp</item>
        <item name="android:gravity">center</item>
        <item name="android:button">@null</item>
        <item name="android:paddingLeft">5dp</item>
        <item name="android:paddingRight">5dp</item>
    </style>
    
    <style name="RadioButton_lager">
        <item name="android:textColor">@drawable/button_text_color</item>
        <item name="android:minHeight">33dp</item>
        <item name="android:minWidth">50dp</item>
        <item name="android:gravity">center</item>
        <item name="android:button">@null</item>
        <item name="android:paddingLeft">5dp</item>
        <item name="android:paddingRight">5dp</item>
    </style>
    
    <style name="Bubble.TextAppearance.Dark" parent="android:TextAppearance.Medium">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#ff7f7f7f</item>
    </style>

    <style name="Bubble.TextAppearance.Light" parent="Bubble.TextAppearance.Dark">
        <item name="android:textColor">#ffeeeeee</item>
    </style>

    <style name="ClusterIcon.TextAppearance" parent="android:TextAppearance">
        <item name="android:textColor">#ffeeeeee</item>
        <item name="android:textSize">16sp</item>
        <item name="android:textStyle">bold</item>
    </style>
    
</resources>
